{
    "compilerOptions": {
        "allowJs": true,
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true
    },
    "include": [
        "src/**/ts/*.ts"
    ],
    "exclude": [
        "app",
        "bin",
        "node_modules",
        "var",
        "vendor",
        "web"
    ]
}
